<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/width_cardMap"
    android:layout_height="@dimen/height_cardMap"
    android:padding="@dimen/padding_cardMap"
    android:background="@drawable/card"
    app:cardCornerRadius="@dimen/cardCornerRadius_cardMap"
    app:cardElevation="@dimen/cardElevation_cardMap">

    <fragment
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</androidx.cardview.widget.CardView>